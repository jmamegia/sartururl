datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Link {
  id        Int      @id @unique @default(autoincrement())
  url       String   @unique
  shortUrl  String   @unique
  createdAt DateTime @default(now())
}
